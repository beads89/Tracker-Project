@using Tracker.Models;

<h1>Current Vendors</h1>

@if (@Model.Count == 0)
{
  <h3>None.</h3>
}

@foreach (Vendors vendors in Model)
{
  <h3><a href="/vendors/@vendors.Id">@vendors.Name</a></h3>
  <ul>
    <li>Notes - @vendors.VendorDescription</li>
  </ul>
}

<p><a href="/">Home</a></p>
<p><a href="/vendors/new">Add a new vendor.</a></p>